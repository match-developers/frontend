{
  "name": "match",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "build": "cross-env NODE_ENV=production babel src --out-dir lib --extensions \".js,.jsx,.ts,.tsx\"",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "cross-env NODE_ENV=development react-native start",
    "test": "jest",
    "setDevelopment": "ENVFILE=.env.development",
    "setStaging": "ENVFILE=.env.staging",
    "setProduction": "ENVFILE=.env.production",
    "androidGradlewClean": "cd android && ./gradlew clean && cd ..",
    "android:dev": "yarn androidGradlewClean && react-native run-android --mode=developmentdebug --appIdSuffix 'development'",
    "android:dev-release": "yarn androidGradlewClean && react-native run-android --mode=developmentrelease --appIdSuffix 'development'",
    "android:staging": "yarn androidGradlewClean && react-native run-android --mode=stagingdebug --appIdSuffix 'staging'",
    "android:staging-release": "yarn androidGradlewClean && react-native run-android --mode=stagingrelease --appIdSuffix 'staging'",
    "android:prod": "yarn androidGradlewClean && react-native run-android --mode=productiondebug",
    "android:prod-release": "yarn androidGradlewClean && react-native run-android --mode=productionrelease",
    "ios:dev": "yarn setDevelopment react-native run-ios --scheme \"MatchDev\"",
    "ios:staging": "yarn setStaging react-native run-ios --scheme \"MatchStg\"",
    "ios:production": "yarn setProduction react-native run-ios --scheme \"MatchProd\"",
    "prepare": "husky",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm test",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.25.7",
    "@react-native-async-storage/async-storage": "^1.22.3",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native-firebase/app": "^18.8.0",
    "@react-native-firebase/auth": "^18.8.0",
    "@react-native-google-signin/google-signin": "^11.0.0",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-native-picker/picker": "^2.7.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "axios": "^1.6.5",
    "lru-cache": "^11.0.1",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-config": "^1.5.1",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-fbsdk-next": "^12.1.2",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-image-picker": "^7.1.1",
    "react-native-maps": "^1.18.0",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^15.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/eslint-parser": "^7.25.7",
    "@babel/plugin-transform-async-generator-functions": "^7.25.7",
    "@babel/plugin-transform-class-properties": "^7.25.7",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.25.7",
    "@babel/plugin-transform-numeric-separator": "^7.25.7",
    "@babel/plugin-transform-object-rest-spread": "^7.25.7",
    "@babel/plugin-transform-optional-catch-binding": "^7.25.7",
    "@babel/plugin-transform-optional-chaining": "^7.25.7",
    "@babel/plugin-transform-react-jsx": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@babel/runtime": "^7.20.0",
    "@eslint/config-array": "^0.18.0",
    "@eslint/js": "^9.12.0",
    "@eslint/object-schema": "^2.1.4",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.37.1",
    "glob": "^11.0.0",
    "globals": "^15.10.0",
    "husky": "^9.0.11",
    "jest": "^29.6.3",
    "lint-staged": "^15.2.2",
    "react-test-renderer": "18.2.0",
    "rimraf": "^6.0.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
